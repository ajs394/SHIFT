<div data-role="page">
  <div data-role="header" data-position="inline">
    <h1>Select Engine</h1>
    <a href="<%= Rho::RhoConfig.start_path %>" class="ui-btn-left" data-icon="back" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>> Back </a>
	</div>

  <div id="content" class="ui-content">
    <form action = "camera">
      
      <p>Select an Engine:</p>
      <select name="engin" id="engin">
        <% @engines.each do |engine| %>
          <option value=<%= engine.object %> > <%= engine.Name %></option>
        <%end%>
      </select>

      <input type="submit" class="standardButton" value="Take Picture" />
    </form>   
	</div>
</div>